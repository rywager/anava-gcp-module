{
  "name": "anava-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end integration tests for Anava Vision System",
  "main": "index.js",
  "scripts": {
    "test": "jest --config=jest.config.js",
    "test:integration": "jest --config=jest.config.js --testPathPattern=tests/integration",
    "test:e2e": "jest --config=jest.config.js --testPathPattern=tests/e2e",
    "test:performance": "jest --config=jest.config.js --testPathPattern=performance",
    "test:load": "artillery run load/scenarios.yml",
    "mock:camera": "node mocks/axis-camera-server.js",
    "mock:all": "npm-run-all --parallel mock:*",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests mocks --ext .js,.ts",
    "format": "prettier --write 'src/**/*.{js,ts}' 'tests/**/*.{js,ts}' 'mocks/**/*.{js,ts}'"
  },
  "keywords": [
    "e2e",
    "integration",
    "testing",
    "webrtc",
    "websocket"
  ],
  "author": "Anava Vision Team",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@types/ws": "^8.5.10",
    "artillery": "^2.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "firebase-admin": "^12.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "node-onvif": "^0.1.7",
    "p-queue": "^7.4.1",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "puppeteer": "^21.7.0",
    "simple-peer": "^9.11.1",
    "socket.io": "^4.6.2",
    "socket.io-client": "^4.6.2",
    "uuid": "^9.0.1",
    "wrtc": "^0.4.7",
    "ws": "^8.16.0",
    "xml2js": "^0.6.2"
  }
}