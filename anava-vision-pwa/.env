# Anava Vision PWA Configuration
REACT_APP_NAME=Anava Vision
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=Professional camera control system with PTZ support

# Orchestrator API Configuration
# Update this URL to point to your camera orchestrator service
REACT_APP_ORCHESTRATOR_URL=http://localhost:3001/api

# Feature Flags
REACT_APP_ENABLE_DEMO_MODE=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_PWA_FEATURES=true

# PWA Configuration
GENERATE_SOURCEMAP=false
REACT_APP_CACHE_DURATION=86400000